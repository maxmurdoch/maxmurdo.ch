{
  "name": "max-murdoch-portfolio-site",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "precommit": "lint-staged",
    "build": "webpack --env=dev",
    "build:prod": "webpack --env=prod",
    "deploy": "yarn run build:prod && yarn run s3 && yarn run bust-cache",
    "s3": "aws s3 cp output/ s3://maxmurdo.ch --recursive",
    "bust-cache": "aws cloudfront create-invalidation --distribution-id E2SJQ5W7KVX6PK --paths /index.html",
    "clean-output": "rimraf output",
    "validate": "npm-run-all --parallel validate-webpack:* lint",
    "validate-webpack:dev": "webpack-validator webpack.config.js --env=dev",
    "validate-webpack:prod": "webpack-validator webpack.config.js --env=prod",
    "copy-files": "cpy input/favicon* output",
    "clean-and-copy": "yarn run clean-output && yarn run copy-files",
    "prestart": "yarn run clean-and-copy",
    "start": "webpack-dev-server --env=dev --content-base 'output'",
    "start:prod": "webpack-dev-server --env=prod --content-base 'output'",
    "prebuild": "yarn run clean-and-copy",
    "prebuild:prod": "yarn run clean-and-copy",
    "lint": "prettier-eslint --write \"./input/**/*.js\"",
    "test": "babel-tape-runner input/**/*test.js | faucet"
  },
  "author": "Max Murdoch, max@maxmurdo.ch",
  "license": "ISC",
  "dependencies": {
    "aos": "^2.2.0",
    "babel-tape-runner": "^2.0.1",
    "chroma-js": "^1.2.2",
    "glamor-reset": "^3.0.0-1",
    "history": "^4.6.2",
    "immutable": "^3.8.1",
    "normalize.css": "^5.0.0",
    "prop-types": "^15.5.8",
    "ramda": "^0.21.0",
    "ramda-repl": "^4.1.1",
    "react": "^15.0.0",
    "react-dom": "^15.2.1",
    "react-fullpage": "^0.1.16",
    "react-headroom": "^2.1.3",
    "react-imation": "^0.5.3",
    "react-motion": "^0.5.0",
    "react-move": "^1.5.4",
    "react-parallax-component": "^1.0.6",
    "react-redux": "^5.0.5",
    "react-router": "^4.0.0",
    "react-router-redux": "next",
    "react-slick": "^0.14.11",
    "react-slide-deck": "^0.2.3",
    "react-track": "^0.4.4",
    "recompose": "^0.23.5",
    "redux": "^3.7.0",
    "redux-devtools-extension": "^2.13.2",
    "reline": "^1.0.0-beta.3",
    "rellax": "^1.0.0",
    "scrollreveal": "^3.3.4",
    "slick-carousel": "^1.6.0",
    "styled-components": "^1.4.3",
    "tumblr.js": "^1.1.1",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.11.0",
    "classnames": "^2.2.5",
    "cpy-cli": "^1.0.1",
    "css-loader": "^0.23.1",
    "eslint": "^3.19.0",
    "eslint-import-resolver-webpack": "^0.4.0",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.11.0",
    "eslint-plugin-react": "^6.10.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "faucet": "^0.0.1",
    "file-loader": "^0.9.0",
    "glamor": "^2.2.0",
    "html-webpack-plugin": "^2.26.0",
    "img-loader": "^1.3.1",
    "inline-style-prefixer": "^3.0.0",
    "lint-staged": "^3.4.1",
    "npm-run-all": "^2.3.0",
    "prettier": "^1.5.2",
    "prettier-eslint": "^6.1.2",
    "prettier-eslint-cli": "^4.0.0",
    "react-hot-loader": "^1.3.0",
    "react-hyperscript-helpers": "^1.2.0",
    "react-router-dom": "^4.0.0",
    "rimraf": "^2.5.3",
    "style-loader": "^0.13.1",
    "stylelint": "^7.9.0",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-processor-styled-components": "^0.0.4",
    "tape": "^4.6.3",
    "url-loader": "^0.5.7",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.0.0-beta",
    "webpack-validator": "^2.2.3"
  },
  "lint-staged": {
    "app/javascript/**/*.js": [
      "prettier-eslint --write",
      "git add"
    ]
  }
}
